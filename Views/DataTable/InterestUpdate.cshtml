@model test.Models.Interest

@{
    Layout = "_Layout";
}

<link href="~/lib/bootswatch/bootstrap.css" rel="stylesheet" />


<h2 style="margin: 50px" class="text-center">Interest</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="InterestUpdate">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdInterest" />
            <div class="form-group">
                <label asp-for="AppliedFrom" class="control-label"></label>
                <input asp-for="AppliedFrom" class="form-control" />
                <span asp-validation-for="AppliedFrom" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppliedTo" class="control-label"></label>
                <input asp-for="AppliedTo" class="form-control" />
                <span asp-validation-for="AppliedTo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Update" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>

<div class="mb-3">
    <a style="width: 90px" class="btn btn-dark btn-sm" asp-action="Index">

        <i class="fas fa-arrow-left"></i>

    </a>
</div>

